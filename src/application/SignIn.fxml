<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.layout.VBox?>

<BorderPane xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.SignInController">
    <center>
        <VBox alignment="CENTER" BorderPane.alignment="CENTER">
            <children>
                <Region VBox.vgrow="ALWAYS" />
                <TextField fx:id="username_input" promptText="Name " VBox.vgrow="ALWAYS" />
            <Region prefHeight="22.0" prefWidth="274.0" />
                <PasswordField fx:id="id_input" promptText="ID (int)" VBox.vgrow="ALWAYS" />
            <Region prefHeight="22.0" prefWidth="274.0" />
            <PasswordField fx:id="password_input" promptText="Password" />
            <Region prefHeight="59.0" prefWidth="274.0" />
            <Button fx:id="Register_button" mnemonicParsing="false" onAction="#register" text="Register" />
                <Region VBox.vgrow="ALWAYS" />
            </children>
        </VBox>
    </center>
    <right>
        <Region prefHeight="400.0" prefWidth="121.0" BorderPane.alignment="CENTER" />
    </right>
    <top>
        <Region BorderPane.alignment="CENTER" />
    </top>
    <bottom>
        <Region prefHeight="43.0" prefWidth="538.0" BorderPane.alignment="CENTER" />
    </bottom>
   <left>
      <Region prefHeight="400.0" prefWidth="143.0" BorderPane.alignment="CENTER" />
   </left>
</BorderPane>
